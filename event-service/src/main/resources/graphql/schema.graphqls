type Query {
    events: [Event!]!
    eventById(id: ID!): Event
    notifications: [Notification!]!
    notificationById(id: ID!): Notification
}

type Event {
    id: ID!
    title: String!
    description: String
    location: String
    eventDate: String!
}

type Notification {
    id: ID!
    title: String!
    message: String
    recipient: String
    sentAt: String!
}